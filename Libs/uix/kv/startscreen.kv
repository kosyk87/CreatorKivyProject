#:kivy 1.9.1

<StartScreen>
    orientation: "vertical"
    ActionBar:
        ActionView:
            id: action_view
            ActionPrevious:
                id: action_previous
                app_icon: root.previous
                title: root.title_previous
                previous_image: root.previous_image
                with_previous: True
                on_press: root.events_callback("previous")
            ActionOverflow:
                id: action_overflow
                overflow_image: root.overflow_image
    ScreenManager:
        id: screen_manager
        Screen:
            name: "screen_1"
            Image:
                source: root.title_image

